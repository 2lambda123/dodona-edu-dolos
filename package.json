{
  "name": "dolos",
  "version": "0.0.1",
  "description": "Plagiarism detection for Dodona based on the Winnowing algorithm",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "debug": "tsc && node inspect dist/app.js",
    "hash": "tsc && node dist/bin/hash.js",
    "test-hash-performance": "tsc && time head -c 1000000 /dev/random | node dist/bin/hash.js > /dev/null",
    "test-winnow-performance": "tsc && time head -c 1000000 /dev/random | node dist/bin/hash.js > /dev/null",
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "build": "tsc",
    "lint": "eslint --max-warnings 0 --ext .ts src/"
  },
  "repository": "git@github.com:dodona-edu/dolos.git",
  "author": "Bart Mesuere <Bart.Mesuere@UGent.be>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^13.7.4",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "ava": "^3.4.0",
    "eslint": "^6.8.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "nprime": "^0.2.1",
    "tree-sitter": "^0.16.0",
    "tree-sitter-c-sharp": "^0.16.1",
    "tree-sitter-java": "^0.16.0",
    "tree-sitter-javascript": "^0.16.0",
    "tree-sitter-python": "^0.16.0"
  }
}
