{
  "name": "dolos",
  "version": "0.0.1",
  "description": "Plagiarism detection for Dodona based on the Winnowing algorithm",
  "main": "dist/dolos.js",
  "bin": "dist/cli.js",
  "scripts": {
    "dolos": "tsc && node dist/cli.js",
    "debug": "tsc && node inspect dist/cli.js",
    "hash": "tsc && node dist/bin/hash.js",
    "test-hash-performance": "tsc && time head -c 1000000 /dev/random | node dist/bin/hash.js > /dev/null",
    "test-winnow-performance": "tsc && time head -c 1000000 /dev/random | node dist/bin/hash.js > /dev/null",
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "build": "tsc && rollup -c",
    "lint": "eslint --ext .ts src/",
    "view:host": "sirv dist/view",
    "view:dev": "rollup -c -w",
    "view:build": "rollup -c"
  },
  "repository": "git@github.com:dodona-edu/dolos.git",
  "author": "Bart Mesuere <Bart.Mesuere@UGent.be>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@pyoner/svelte-types": "^3.4.4-2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/benchmark": "^1.0.33",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "ava": "^3.8.2",
    "benchmark": "^2.1.4",
    "d3": "^5.16.0",
    "eslint": "^6.8.0",
    "node-sass": "^4.14.1",
    "nyc": "^15.0.1",
    "rollup": "^2.13.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sirv-cli": "^0.4.6",
    "svelte": "^3.20.1",
    "svelte-material-ui": "^1.0.0-beta.21",
    "svelte-preprocess": "^3.7.1",
    "ts-node": "^8.9.1",
    "tslib": "^2.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "cliui": "^6.0.0",
    "commander": "^5.1.0",
    "csv-stringify": "^5.5.0",
    "express": "^4.17.1",
    "nprime": "^0.2.1",
    "tree-sitter": "^0.16.1",
    "tree-sitter-c-sharp": "^0.16.1",
    "tree-sitter-java": "^0.16.0",
    "tree-sitter-javascript": "^0.16.0",
    "tree-sitter-python": "^0.16.0"
  }
}
